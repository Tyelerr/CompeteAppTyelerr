# App Store Connect API Configuration
# Get these credentials from App Store Connect > Users and Access > Keys

# Your Issuer ID (found in App Store Connect > Users and Access > Keys)
APP_STORE_CONNECT_ISSUER_ID=your-issuer-id-here

# Your Key ID (the ID of your API key)
APP_STORE_CONNECT_KEY_ID=your-key-id-here

# Path to your private key file (.p8 file downloaded when creating the API key)
APP_STORE_CONNECT_PRIVATE_KEY_PATH=./keys/AuthKey_KEYID.p8

# Optional: Bundle ID (defaults to com.tyelerr.app if not specified)
BUNDLE_ID=com.tyelerr.app

# Optional: Transporter settings
TRANSPORTER_VERBOSE=false
TRANSPORTER_DRY_RUN=false
